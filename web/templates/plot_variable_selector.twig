<div>
        <h4>Select 2 Variables to Plot</h4>
        <div class="row center-block" style="padding-top:3px;">
            <form method="get" role="form" id="formplotdata">
                <select data-placeholder="Choose OBD2 data..." multiple class="chosen-select"
                        style="width:100%;" id="plot_data" onsubmit="onSubmitIt"
                        name="plotdata[]">
                    <option value=""></option>
                    {% for pid_id, pid_name in pid_names %}
                        <option value="{{ pid_id }}"{% if plot_data[pid_id] is defined %} selected{% endif %}>{{ pid_name }}</option>
                    {% endfor %}
                </select>

                <div align="center" style="padding-top:6px;">
                    <input class="btn btn-info btn-sm" type="submit" value="Plot!">
                </div>
                <input type="hidden" name="id" value="{{ session_data.sid }}">
            </form>
        </div>
</div>